{
  "name": "badger-desktop",
  "productName": "Badger Desktop",
  "version": "0.0.0",
  "description": "My Electron application description",
  "type": "module",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "rimraf out && electron-vite build",
    "package": "rimraf dist && yarn build && electron-builder build -c electron-builder.config.cjs",
    "lint": "eslint src/",
    "prettier:check": "prettier --check '**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write '**/*.{ts,tsx}'",
    "prettify": "yarn prettier:fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:e2e": "E2E_TEST=true playwright test",
    "test:e2e:debug": "E2E_TEST=true BADGER_LOG_LEVEL=trace DEBUG='pw:browser*,mows*' playwright test",
    "test:e2e:ui": "E2E_TEST=true playwright test --ui",
    "microserver": "tsx dev/microserver/index.ts",
    "microserver:watch": "tsx --watch dev/microserver/index.ts",
    "prepare": "husky"
  },
  "keywords": [],
  "author": {
    "name": "Marks Polakovs",
    "email": "marks@markspolakovs.me"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*": "prettier --ignore-unknown --write"
  },
